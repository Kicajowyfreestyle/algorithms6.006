   "source": [
    "### computes cube root of x\n",
    "def cbrt(x):\n",
    "    ret = float(x)\n",
    "    first = ret/3\n",
    "    for i in range(50):\n",
    "        ret = 2*ret/3 + first/(ret*ret)\n",
    "    return ret"
   ]
